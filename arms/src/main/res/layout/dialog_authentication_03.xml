<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="250dp"
    android:layout_height="130dp"
    android:background="@drawable/login_head_dialog_border"
    android:orientation="vertical">
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ImageView
        android:id="@+id/dialog_authentication_03_cancel"
        android:layout_marginTop="10dp"
        android:layout_marginRight="10dp"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:padding="5dp"
        android:src="@drawable/dialog_authentication_02_delete"
        android:layout_alignParentRight="true"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:orientation="vertical"
        android:paddingLeft="5dp"
        android:paddingRight="5dp">

        <RelativeLayout
            android:layout_width="250dp"
            android:layout_height="0dp"
            android:layout_weight="1">

            <TextView
                android:id="@+id/dialog_authentication_01_photo"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="亲爱的商家，您有未填写的信息，请全部填写完提交审核"
                android:textColor="@color/text"
                android:textSize="18sp" />

        </RelativeLayout>






    </LinearLayout>
    </RelativeLayout>
</LinearLayout>
